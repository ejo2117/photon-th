{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": false,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "Node",
		"baseUrl": "./src/frontend",
		"paths": {
			"@components/*": ["components/*"],
			"@services/*": ["services/*"]
		}
	}
}
